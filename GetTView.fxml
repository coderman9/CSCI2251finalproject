<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.GetTenantController">
   <center>
      <FlowPane hgap="40.0" prefHeight="200.0" prefWidth="200.0" vgap="50.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="nameField" promptText="Name" />
            <Button mnemonicParsing="false" onAction="#submitGetTenant" prefWidth="149.0" text="Find Tenant" />
            <ListView fx:id="displayList" prefHeight="200.0" prefWidth="200.0" />
         </children>
         <BorderPane.margin>
            <Insets left="10.0" top="20.0" />
         </BorderPane.margin>
      </FlowPane>
   </center>
   <top>
      <Label text="Add a new Tenant" BorderPane.alignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </top>
</BorderPane>
