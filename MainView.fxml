<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="640.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.MainViewController">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
            <Button mnemonicParsing="false" onAction="#goAddRental" text="New Property" />
          <Button mnemonicParsing="false" onAction="#goGetRentalByID" text="Rental ID Lookup" />
            <Button mnemonicParsing="false" onAction="#goGetRentalByType" text="Availability" />
            <Button mnemonicParsing="false" onAction="#goSetRental" text="Reservations" />
            <Button mnemonicParsing="false" onAction="#goAddTenant" text="New Tenancy" />
            <Button mnemonicParsing="false" onAction="#goGetTenant" text="Tenant Lookup" />
            <Button mnemonicParsing="false" onAction="#goGetBilling" text="Billing Lookup" />
            <Button mnemonicParsing="false" onAction="#goSetBilling" text="Payment" />
        </items>
      </ToolBar>
   </top>
</BorderPane>
